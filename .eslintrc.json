{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "overrides": [
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "parserOptions": {
        "project": "./tsconfig.json"
      },
      "rules": {
        "no-unused-vars": "warn",
        "no-undef": "warn",
        "no-unused-labels": "warn",
        "no-case-declarations": "off",
        "no-constant-condition": "off"
      }
    }
  ]
}